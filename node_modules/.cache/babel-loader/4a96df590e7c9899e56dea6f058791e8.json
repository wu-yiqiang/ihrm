sourcesContent":["<template>\r\n  <div>\r\n    <el-upload\r\n        class=\"avatar-uploader\"\r\n        action=\"#\"\r\n        :show-file-list=\"false\"\r\n        :on-success=\"handleAvatarSuccess\"\r\n        :file-list=\"fileList\"\r\n        :before-upload=\"beforeAvatarUpload\" :limit=\"1\" :class=\"{disable: fileComputed}\" :on-preview=\"preview\" :on-remove=\"handleRemove\">\r\n        <img v-if=\"imageUrl\" :src=\"imageUrl\" class=\"avatar\">\r\n        <i v-else class=\"el-icon-plus avatar-uploader-icon\" ></i>\r\n    </el-upload>\r\n    <el-dialog :visible.sync=\"dialogVisible\" title=\"图片预览\">\r\n      <img width=\"100%\" :src=\"imghUrl\" alt=\"\">\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'ImageUpload',\r\n  data() {\r\n    return {\r\n      imageUrl: '',\r\n      fileList:[{url:'https://ss3.bdstatic.com/70cFv8Sh_Q1YnxGkpoWK1HF6hhy/it/u=800205596,459974077&fm=26&gp=0.jpg'}],\r\n      dialogVisible: false,\r\n      imghUrl: null\r\n    };\r\n  },\r\n  computed: {\r\n    fileComputed() {\r\n      return this.fileList.length === 1\r\n    }\r\n  },\r\n  methods: {\r\n    handleAvatarSuccess(res, file) {\r\n      this.imageUrl = URL.createObjectURL(file.raw);\r\n    },\r\n    beforeAvatarUpload(file) {\r\n      const isJPG = file.type === 'image/jpeg';\r\n      const isLt2M = file.size / 1024 / 1024 < 2;\r\n\r\n      if (!isJPG) {\r\n        this.$message.error('上传头像图片只能是 JPG 格式!');\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error('上传头像图片大小不能超过 2MB!');\r\n      }\r\n      return isJPG && isLt2M;\r\n    },\r\n    preview(file) {\r\n      this.imageUrl = file.url\r\n      this.dialogVisible = true\r\n    },\r\n    handleRemove () {\r\n      \r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style lang='scss' >\r\n  .avatar-uploader .el-upload {\r\n    border: 1px dashed #d9d9d9;\r\n    border-radius: 6px;\r\n    cursor: pointer;\r\n    position: relative;\r\n    overflow: hidden;\r\n  }\r\n  .a