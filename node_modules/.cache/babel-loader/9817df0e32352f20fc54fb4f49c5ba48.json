ees/${id}/personalInfo`\r\n  });\r\n}\r\n/** *\r\n * 更新用户详情的基础信息\r\n * **/\r\nexport function updatePersonal(data) {\r\n  return request({\r\n    url: `/employees/${data.userId}/personalInfo`,\r\n    method: \"PUT\",\r\n    data\r\n  })\r\n}\r\n\r\n/** **\r\n * 获取用户的岗位信息\r\n *\r\n * ****/\r\nexport function getJobDetail(id) {\r\n  return request({\r\n    url: `/employees/${id}/jobs`\r\n  });\r\n}\r\n\r\n/**\r\n * 保存岗位信息\r\n * ****/\r\nexport function updateJob(data) {\r\n  return request({\r\n    url: `/employees/${data.userId}/jobs`,\r\n    method: \"put\",\r\n    data\r\n  });\r\n}\r\n"]}]}